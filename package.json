  },
  "main": "./lib/googleapis",
  "repository": {
    "type": "git",
    "url": "https://github.com/zoomsphere/google-api-nodejs-client.git"
  },
  "contributors": [
    {
      "name": "Burcu Dogan",
      "email": "jbd@google.com"
    },
